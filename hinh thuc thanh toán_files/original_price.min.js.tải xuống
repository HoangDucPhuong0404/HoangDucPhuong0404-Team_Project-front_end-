define(['Magento_Checkout/js/view/summary/abstract-total'],function(viewModel){'use strict';return viewModel.extend({defaults:{displayArea:'after_details',template:'Magenest_CustomCheckout/checkout/summary/item/price/original_price'},getOriginalPrice:function(quoteItem){return this.getFormattedPrice(quoteItem['extension_attributes']['original_price']);},isShowOriginalPrice:function(quoteItem){let ex=quoteItem['extension_attributes'];if(typeof ex!='undefined'&&typeof ex['original_price']!='undefined'&&parseInt(ex['original_price'])!=parseInt(quoteItem['price'])){return true;}
return false;}});});