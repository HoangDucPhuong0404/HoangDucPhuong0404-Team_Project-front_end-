define(['jquery','Magento_Checkout/js/view/payment/default','Magento_Checkout/js/model/payment/additional-validators','Magento_Checkout/js/action/redirect-on-success'],function($,Component,additionalValidators,redirectOnSuccessAction){'use strict';return Component.extend({defaults:{template:'Magenest_VNPAY/payment/form',},getCode:function(){return'vnpay';},placeOrder:function(data,event){var self=this;if(event){event.preventDefault();}
if(this.validate()&&additionalValidators.validate()){this.isPlaceOrderActionAllowed(false);this.getPlaceOrderDeferredObject().fail(function(){self.isPlaceOrderActionAllowed(true);}).done(function(orderID){$.ajax({url:window.checkoutConfig.payment[self.getCode()].infoUrl,dataType:"json",type:'POST',showLoader:true,data:{order_id:orderID}}).done(function(url){console.log(url);window.location.replace(url);}).fail(function(err){console.log(err);});self.afterPlaceOrder();});return true;}
return false;}});});