require(["jquery","Magento_Ui/js/modal/modal"],function($,modal){var options={type:'popup',responsive:true,title:$.mage.__('Forgotten password'),modalClass:'popup-forgotten-password',buttons:[{class:'',click:function(){this.closeModal();}}]};var forgotModal=modal(options,$('#forgot-password-modal'));$("#open-forgot-action").click(function(){$('#forgot-password-modal').modal('openModal');});});