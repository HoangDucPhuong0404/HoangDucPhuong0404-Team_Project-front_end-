define(["jquery","jquery-ui-modules/widget",'mage/validation'],function($){'use strict';$.widget('mage.telephonevn',{options:{checkboxSubscribe:'[id=is_checkbox_agree_bottom] input'},_create:function(){this._bind();},_bind:function(){this._on(this.element,{'change':this._validateField,'keyup':this._validateField,'paste':this._validateField});},_validateField:function(){var isValid=$.validator.validateSingleElement(this.element);if(isValid){$(this.options.checkboxSubscribe).removeAttr("disabled");}else{$(this.options.checkboxSubscribe).attr("disabled",true).prop('checked',false);}}});return $.mage.telephonevn;});