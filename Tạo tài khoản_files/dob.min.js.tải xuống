define(["jquery",'Magento_Ui/js/modal/modal','Magento_Ui/js/modal/alert',"mage/url","mage/storage",'mage/translate','tabs','mage/mage',"!domReady"],function($,modal,alert,urlBuilder,storage,$t){"use strict";$.widget("mage.MagenestDob",{options:{disableSelect:false},_create:function(){var self=this;self.initFieldDob();$(".dateselect").change(function(){var current_value=$(this).val(),date=null;var related_field_id=$(this).attr('rel');$("#"+related_field_id).val(current_value);date=$('#day').val()+'/'+$('#month').val()+'/'+$('#year').val()
$('#dob').val(date)});this._bind();},_bind:function(){},initFieldDob:function(){var self=this;$(".dateselect").each(function(){var related_field_id=$(this).attr('rel');var current_value=$("#"+related_field_id).val();$(this).val(current_value);if(self.options.disableSelect){$(this).attr('disabled',true);}});}});return $.mage.MagenestDob;});