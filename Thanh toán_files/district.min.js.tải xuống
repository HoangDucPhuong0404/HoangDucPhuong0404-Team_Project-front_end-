define(['underscore','uiRegistry','Magento_Ui/js/form/element/select'],function(_,registry,Select){'use strict';return Select.extend({defaults:{skipValidation:false,captionValue:'tn',imports:{update:'${ $.parentName }.city_id:value'}},setDifferedFromDefault:function(){this._super();registry.async(this.parentName+'.'+'district')(function(element){element.setVisible(false);});},onUpdate:function(){var self=this,option=_.find(this.options(),function(item){return item.value==self.value();});if(!_.isUndefined(option)){this.source.set(this.dataScope.replace('_id',''),option.full_name);}}});});